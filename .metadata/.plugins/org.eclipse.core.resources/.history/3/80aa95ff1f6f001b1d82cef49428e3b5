package entities;

public class Rectangle {
	
	public double width;
	public double height;
	
	public double area() {
		return width * height;
	}
	
	public double perimeter() {
		return width * 2 + width * 2;
	}
	
	public double diagonal() {
		double p = (a * a) + (b * b);
		return Math.sqrt(p);
	}

}
